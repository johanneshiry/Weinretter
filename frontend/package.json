{
  "name": "weinretter",
  "version": "1.0.0",
  "description": "WeinRetter - Hol dir dein Lieblingslokal ins Wohnzimmer",
  "author": "Das Weinretter.de Team",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "test": "jest",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "format": "prettier --write '{assests,components,layouts,pages,plugins,static,static,store}/**/*.{json,css,ts,tsx,js,vue}'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,vue}": [
      "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0",
    "leaflet": "^1.6.0",
    "leaflet-geosearch": "^2.7.0",
    "nuxt": "^2.0.0",
    "nuxt-leaflet": "0.0.21",
    "vue-typer": "^1.2.0",
    "vue2-leaflet": "^2.5.2",
    "vue2-leaflet-geosearch": "^1.0.6",
    "vue2-leaflet-markercluster": "^3.1.0"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.0",
    "@types/leaflet": "^1.5.12",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.3",
    "jest": "^24.1.0",
    "lint-staged": "^10.0.9",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "vue-jest": "^4.0.0-0"
  }
}
